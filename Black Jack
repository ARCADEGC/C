#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <math.h>

int value(){
    int r = rand() % 13;
    return r;
}

int color(){
    int r = rand() % 4;
    return r;
}



int main() {
    srand(time(NULL));
    
    for(int i = 0; i <= 20; i++){
        printf("%i of %i\n", value(), color());    
    }
    
    return 0;
}
