#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <math.h>

int opponent(){
    int r = (rand() % 8) + 18;
    return r;
}

int value(){
    int r = (rand() % 13) + 1;
    if(r == 1){
        return r = 65;
    }
    if(r == 11){
        return r = 74;
    }
    if(r == 12){
        return r = 81;
    }
    if(r == 13){
        return r = 75;
    }
    return r;
}

int color(){
    int r = (rand() % 4) + 1;
    if(r == 1){
        return r = 83;
    }
    if(r == 2){
        return r = 72;
    }
    if(r == 3){
        return r = 67;
    }
    return r = 68;
}

int main() {
    srand(time(NULL));
    int OP = opponent();
    
    for(int i = 0; i <= 1; i++){
        int C = color();
        int F = value();
        if(F == 65 || F == 74 || F == 81 || F == 75){
            printf("%c of %c\n", F, C);
        }
        else{
            printf("%i of %c\n", F, C);
        }
    }
    
    printf("%i", OP);
    
    return 0;
}
