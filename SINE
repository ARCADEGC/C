#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {

     int sinusi;
    int x;

    for(x = 0; x <= 360; x+=15){
        sinusi = round((sin(x*3.1415/180))*20)+20;
        do{
            printf(" ");
            sinusi--;
        }while(sinusi>0);
        printf("0\n");
    }
    return 0;
}
